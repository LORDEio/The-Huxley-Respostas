#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int ph;
    scanf("%d", &ph);
    
    if (ph<7)
    {
        printf("Acida");
    }
    if (ph>7)
    {
        printf("Basica");
    }
    if (ph==7)
    {
        printf("Neutra");
    }
    
    return 0;
}
