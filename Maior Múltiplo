#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int M, N, res;
    scanf("%d", &M);
    scanf("%d", &N);
    
    if(N > M)
    {
        while(N % M != 0)
            N--;
        
        printf("%d\n", N);
    }
    else
        printf("sem multiplos menores que %d\n", N);
    return 0;
}
