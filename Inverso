#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
            int n, v[100], i;
            scanf("%d", &n);
            
            for(i = 0; i< n; i++)
            {
                scanf("%d", &v[i]);
            }
            
            for(i = n-1; i >= 0; i--)
            {
                    if(i < n-1)
                    {
                        printf(" ");
                    }
                    printf("%d", v[i]);
                    
            }
            printf("\n");
	return 0;
}
