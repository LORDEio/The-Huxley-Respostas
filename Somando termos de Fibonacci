#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int fib(int n)
   {
       int z;
   if (n == 1) return (1);
   else
   if (n == 2) return (1);
   else return fib(n - 1) + fib(n - 2);
   }

int main(){
    int resultado=0, a, k, j;
    scanf("%d", &a);
    for(j=0;j<a;j++)
    {
        scanf("%d", &k);
        resultado += fib(k);
    }
    printf("%d", resultado);
    return 0;
}
