#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(void) {
long long int N;
scanf("%lld", &N);
if (N == 0)
{
    printf("NULO\n");
}
else
{
    if(N<0)
    {
        if(llabs(N) % 2 == 0)
        {
            printf("NEGATIVO PAR\n");
        }
        else
        {
            printf("NEGATIVO IMPAR\n");
        }
    }
    else
    {
        if(N % 2 == 0)
        {
            printf("POSITIVO PAR\n");
        }
        else
        {
            printf("POSITIVO IMPAR\n");
        }
    }
}
  return 0;
}
