#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

void troca(int *x, int *y);

int main() 
{
    int a, b;
    scanf("%d%d", &a, &b);
    printf("a: %d b: %d\n", a, b);
    troca (&a, &b);
    printf("a: %d b: %d\n", a, b);
    return 0;
}

    void troca(int *x, int *y)
{
    int aux = *x;
    *x = *y;
    *y= aux;
}
