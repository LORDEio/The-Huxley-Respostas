#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int A, B, C, D, resultado1, resultado2, diferenca;
     scanf("%d", &A);
     scanf("%d", &B);
     scanf("%d", &C);
     scanf("%d", &D);
     
     resultado1 = A * B;
     resultado2 = C * D;
     diferenca = resultado1 - resultado2;
     
     printf("DIFERENCA = %d", diferenca);
	return 0;
}
