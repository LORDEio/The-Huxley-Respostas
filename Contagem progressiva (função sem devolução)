#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

void linhas(int x)
{
    int linha, i, k;
    for(i=1;i<=x;i++)
    {
        linha = x-x+i;
        for(k=0;k<linha;k++)
        {
            printf("%d", linha);
            if(k<linha-1)
                printf("-");
        }
        printf("\n");
    }
    
}

int main() {
    int n;
    scanf("%d", &n);
    linhas(n);
	return 0;
}
