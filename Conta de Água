#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int N, valor;
  scanf("%d", &N);
  if(N <= 10)
    printf("7\n");
  else
  {
    if(N > 101)
    {
      valor = 167 + (N - 100) * 5;
      printf("%d", valor);
    }
    else
    {
      if(N > 31)
      {
        valor = 27 + (N - 30) * 2;
        printf("%d", valor);
      }
      else
      {
        valor = 7 + (N - 10);
        printf("%d", valor);
      }
    }
  }
    return 0;
}
