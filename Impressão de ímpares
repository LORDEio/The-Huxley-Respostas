#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {


    int n1, n, i, i2;

    n1=0;
    i2=0;
    i=0;
        scanf("%d", &n);
        do{
            n1= i2 % 2;
            if(n1!=0){
                i++;
                printf("%d\n", i2);
            }
            i2++;
        }while(n>i);
	return 0;
}
