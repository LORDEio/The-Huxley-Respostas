#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {


    int n1, n2, n3;

            scanf("%d %d %d", &n1, &n2, &n3);

                    if((n1+n2)>n3){
                            {

                            if(n1==n2 && n2==n3){
                                printf("Equilatero");
                                
                            }else{
                            if(n1==n2 || n2==n3 || n3==n1){
                                printf("Isosceles");
                                
                            }else{
                            printf("Escaleno");}
                                }
                            }

                                }else 
                                 printf("Nao eh triangulo");
	return 0;
}
