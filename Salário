#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n, h;
    float v, s;
    
    scanf("%d\n", &n);
    scanf("%d\n", &h);
    scanf("%f\n", &v);
    
    s = (float)v * h;
    
    printf("NUMBER = %d\n", n);
    printf("SALARY = R$ %.2f", s);
    
    
	return 0;
}
