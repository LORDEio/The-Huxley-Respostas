#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int v[10], i=0, j=0;
    
    for(i = 0; i<10; i++)
       scanf("%d", &v[i]);
       
       
    for(i = 0; i<10; i++)
    {
        if(v[i] == v[9])
        {
            j = j + 1;
        }
        
    }
    printf("O numero %d apareceu %d vezes", v[9], j);
    
	return 0;
}
